@{
  Layout = "_Layout";
}

<h1>Band Tracker</h1>
<hr />
<h2>Venues:</h2>
@if (Model.Count != 0)
{
  <ol>
    @foreach (var venue in Model)
    {
      <li><a href='/venues/@venue.GetId()'>@venue.GetVenueName()</a></li>
    }
  </ol>
}
<h4><a href="/venues/new">Add a new Venue</a></h4>
<h4><a href="/">Return to Home</a></h4>
