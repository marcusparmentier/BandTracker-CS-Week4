@{
  Layout = "_Layout";
}

<h1>Band Tracker</h1>
<hr />
<h2>Venues:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var venue in Model)
    {
      <li><a href='/venues/@venue.GetId()'>@venue.GetVenueName()</a> | <a href="/venues/@venue.GetId()/update">Update</a> | <a href="/venue/@venue.GetId()/delete">Delete</a></li>
    }
  </ul>
}
<br>
<h4><a href="/venues/new">Add a new Venue</a></h4>
<h4><a href="/">Return to Home</a></h4>
