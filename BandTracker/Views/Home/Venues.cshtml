@{
  Layout = "_Layout";
}

<h1>Band Tracker</h1>
<hr />

<div class="well">
  <h2>Venues:</h2>
  @if (Model.Count != 0)
  {
    <ul>
    @foreach (var venue in Model)
    {
      <li><a href='/venues/@venue.GetId()'>@venue.GetVenueName()</a> || <a href="/venues/@venue.GetId()/update">Update</a> | <a href="/venues/@venue.GetId()/delete">Delete</a></li>
    }
    </ul>
  }
</div>

<div class="well">
  <h2><a href="/venues/new">Add a new Venue</a></h2>
</div>

<div class="well">
  <h4><a href="/">Return to Home</a></h4>
</div>
